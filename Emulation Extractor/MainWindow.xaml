<Window x:Class="Emulation_Extractor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Emulation_Extractor"
        mc:Ignorable="d"
        Title="Emulation Extraction" Height="450" Width="800">
    <Grid Background="{StaticResource RetroGrey}">
        <StackPanel Margin="5,5,5,5">
            <Button Click="StartBtn_Click" Margin="5">Start</Button>
            <Button Click="EmulatorsBtn_Click" Margin="5">Emulators</Button>
        </StackPanel>
    </Grid>
</Window>
