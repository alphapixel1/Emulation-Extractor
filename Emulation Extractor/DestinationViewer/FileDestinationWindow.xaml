<Window x:Class="Emulation_Extractor.FileDestinationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Emulation_Extractor"
        mc:Ignorable="d"
        Title="FileDestinationWindow" Height="450" Width="800" Background="{StaticResource RetroGrey}"
        Loaded="Window_Loaded">
    <ScrollViewer>
        <StackPanel x:Name="MainStack">
            <TextBlock FontSize="20" HorizontalAlignment="Center" Foreground="{StaticResource TextColor}">Destinations</TextBlock>
        </StackPanel>
    </ScrollViewer>
</Window>

    